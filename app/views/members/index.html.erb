<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Website</th>
      <th>Friend count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

<iframe class="domain-widget-iframe domain-widget-testaff" src="//www.betabh.com/web-hosting/iframe_banner?banner_name=qa_test&aff_track=testaff&main_site=www.betabh.com" width="430" height="288" target="_blank"> </iframe>


  
  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.website %></td>
        <td><%= member.friend_count %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
